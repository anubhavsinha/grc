{{! dashboard object listing}}
<ul class="tree-structure simple single-row-tabs">
{{#list}}
  <li>
    {{#pack this tooltip_view=tooltip_view object_type=object_type object_display=object_display object_route=object_route}}
    <a data-popover-trigger="sticky-hover" data-popover-trigger="sticky-hover" data-sticky-popover="true" data-content="{{render tooltip_view}}">
      {{{withattr "href" "/{object_route}/{id}" "data-original-title" "<div class='row-fluid'><div class='span10'><h3><i class='grcicon-{object_type}-color'></i>{slug} <span>{title}</span></h3></div><div class='span2'><a class='btn btn-mini pull-right' href='/{object_route}/{id}' rel='tooltip' data-original-title='View/Edit {object_display}'><i class='grcicon-goto'></i></a></div></div>" }}}
      <div class="row-fluid">
        <div class="span12">
          <div class="item-identifier">
            <span class="key">
              <span>{{prefix}}</span> <span>{{slug}}</span>
            </span>
            <span class="name">
              {{title}}
            </span>
          </div>
        </div>
      </div>
    </a>
    {{/pack}}
  </li>
{{/list}}
</ul>